# Database 
DATABASE_URL=postgresql://swaadly_user:YOUR_DB_PASSWORD@/swaadly?host=/cloudsql/PROJECT_ID:REGION:swaadly-db
DATABASE_MIGRATION_URL=postgresql://postgres:.2000Debo@localhost:5432/swaadly-postgres-db

# Redis
REDIS_HOST=10.x.x.x  
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT Secrets 
JWT_ACCESS_SECRET=<generate-256-bit-secret>
JWT_REFRESH_SECRET=<generate-256-bit-secret>
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# OTP
OTP_LENGTH=6
OTP_EXPIRES_IN=300
OTP_MAX_ATTEMPTS=3

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Resend (Email)
RESEND_API_KEY=your-resend-api-key
RESEND_FROM_EMAIL=noreply@yourdomain.com

# Fast2SMS
FAST2SMS_API_KEY=your-fast2sms-api-key
FAST2SMS_SENDER_ID=SWADLY
FAST2SMS_TEMPLATE_ID=template-id

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX_REQUESTS=100

# Throttler
THROTTLER_TTL=60000
THROTTLER_LIMIT=100

# Application
APP_PORT=5000
NODE_ENV=production
FRONTEND_URL=https://yourdomain.com
INTERNAL_API_KEY=<generate-strong-random-key-min-32-chars>

# GCS
GCS_BUCKET_NAME=swaadly-uploads-gcp

# SHIPMOZO
SHIPMOZO_PUBLIC_KEY=your-secreet-public-key
SHIPMOZO_PRIVATE_KEY=your-secreet-private-key
SHIPMOZO_API_URL=https://shipping-api.com/app/api/v1